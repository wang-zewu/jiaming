var flagRequest = 0;

function count() {
var minute = parseInt((condition-current)/60);
var second = (condition-current)%60;
if(condition == -1) {
jq("#online_time").html('今日奖励已领完');
return;	
}
if(minute < 10){
minute = '0'+ minute;
}
if(second < 10){
second = '0'+ second;
}
jq("#online_time").html(minute+":"+second); 
var width = parseInt((current/condition)*120);
jq("#online_bg").css('width',width+'px');
current++;
if(current <= condition ){
setTimeout(count,1000);
flagRequest++;
if(flagRequest >= 1) {
flagRequest = 0;
}
} else{
jq("#online_time").html('点击领取奖励');
changeColor();
changeText();
